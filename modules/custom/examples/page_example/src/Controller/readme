    $query = \Drupal::database()->select('node__body', 'n');
    $query->fields('n', ['entity_id']);
    $query->addField('d7', 'body_summary');
    $query->join('d7_field_data_body', 'd7', 'd7.body_value = n.body_value');
    $results = $query->execute()->fetchAllKeyed();


    foreach ($results as $key => $value) {

      if($value) {

        $query = \Drupal::database()->insert('node__field_teaser_text');
        $query->fields([
            'bundle',
            'entity_id',
            'revision_id',
            'langcode',
            'delta',
            'field_teaser_text_value'
        ]);
        $query->values([
            'article',
            $key,
            $key,
            'uk',
            0,
            $value
        ]);
        $query->execute();

      }

    }

    SELECT node.title, file_managed.filename
    FROM node
        INNER JOIN field_data_field_cover ON node.nid = field_data_field_cover.entity_id
        INNER JOIN file_managed ON file_managed.fid  = field_data_field_cover.field_cover_fid





    $query = \Drupal::database()->select('d7_title_cover', 'd7');
    $query->fields('d7', ['title', 'filename']);
    $results = $query->execute()->fetchAllKeyed();

    foreach ($results as $key => $value) {
      if($value) {
        dsm($key);
        dsm($value);


        $query = \Drupal::database()->select('node_field_data', 'n');
        $query->addField('n', 'nid');
        $query->condition('n.title', $key);
        $query->range(0, 1);
        $query->orderBy('created', 'DESC')
        $nid = $query->execute()->fetchField();

        $query = \Drupal::database()->select('media_field_data', 'f');
        $query->addField('f', 'mid');
        $query->condition('f.name', $value);
        $query->range(0, 1);
        $mid = $query->execute()->fetchField();

        dsm($nid);
        dsm('-');
        dsm($mid);
        dsm('--------------------------');

        if($nid && $mid) {

          $query = \Drupal::database()->insert('node__field_teaser_media');
          $query->fields([
              'bundle',
              'entity_id',
              'revision_id',
              'langcode',
              'delta',
              'field_teaser_media_target_id'
          ]);
          $query->values([
              'article',
              $nid,
              $nid,
              'uk',
              0,
              $mid
          ]);
          $query->execute();



        $query = \Drupal::database()->insert('node_revision__field_teaser_media');
        $query->fields([
            'bundle',
            'entity_id',
            'revision_id',
            'langcode',
            'delta',
            'field_teaser_media_target_id'
        ]);
        $query->values([
            'article',
            $nid,
            $nid,
            'uk',
            0,
            $mid
        ]);
        $query->execute();
      }



      }
    }



     $query = \Drupal::database()->select('d7_title_path', 'd7');
        $query->fields('d7', ['title', 'path']);
        $results = $query->execute()->fetchAllKeyed();

        foreach ($results as $key => $value) {
          if ($value) {
            dsm($key);
            dsm($value);

            $query = \Drupal::database()->select('node_field_data', 'n');
            $query->addField('n', 'nid');
            $query->condition('n.title', $key);
            $query->range(0, 1);
            $nid = $query->execute()->fetchField();

            if($nid) {
              $query = \Drupal::database()->insert('url_alias');
              $query->fields([
                'source',
                'alias',
                'langcode',
              ]);
              $query->values([
                '/node/'.$nid,
                '/'.$value,
                'uk',
              ]);
              $query->execute();
            }

          }
        }




$query = \Drupal::database()->select('taxonomy_term__field_archived', 'a');
      $query->fields('a', ['entity_id', 'field_archived_value']);
      $query->condition('a.field_archived_value', '1');
      $results = $query->execute()->fetchAllKeyed();
      $archived = [];

      foreach ($results as $key => $value) {
         array_push($archived, $key);
      }

      $query = \Drupal::database()->select('taxonomy_term_field_data', 't');
      $query->fields('t', ['tid', 'name']);
      $query->condition('t.vid', 'projects');
      $query->condition('t.tid', $archived, 'NOT IN');
      $results = $query->execute()->fetchAllKeyed();

      $dropdown_array = array('' => t('- Any -'));

      foreach ($results as $key => $value) {
         $dropdown_array[$value] = $value;
      }